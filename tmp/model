fin
end_ballance
 
 hk 1  npyrc 0 input /gl pyrc 0
 pyrclac 1 -npyrc    0 output pyrc
 lacin  2 -y[nlac] nlacc 0 input lac lacc 0
 laccpyr 2  -nlacc npyr  0 input lacc pyr 0 
 pyrclacc 2 -npyrc   nlacc  0 input pyrc lacc  0
 pyrdcm 2 -npyrc npyr 0 input pyrc pyr  0
 pdh 2 -npyr  ncoa 0 irr cutfirst pyr coa
 citakg 2 -ncit  nakg 1 irr icdh cit akg      
 akgsuc 2 -nakg    nsuc 1 irr decarb akg suc      
 sucmal 2 -nsuc nmal 1 input suc mal  0
 pc 2 -npyr  nmal 0 irr carb pyr mal 
 malicm 2 -nmal  npyr 0 irr decarb mal pyr
 oacd 2 -noac nmal 0 input oac mal  0
 akgdcm 2 -nakgc nakg 0 input akgc akg 0
 liase 2 -ncit  noac 0 r3met split cit oac coa fluxes[liase] 0
 akgcit1 2 -nakgc   ncit 0 irr icdhr akgc cit
 gln_in 2 -y[ngln] nakgc  0 input gln akgc  0
 ala_o 1 -npyrc  0 output pyrc 
 cs0 3 -nmal -ncoa ncit 1 r3met condence cit mal coa fluxes[cs0] 0
  D 1 - 0 0 0 0 0
 rdt 1 - 0 0 0 0 0
fin
